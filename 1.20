n = int(input('Enter integer:\n'))
print('You entered:',n)
square = n * n
cube = square * n
print('{0} squared is {1}'.format(n, square))
print('And {0} cubed is {1} !!'.format(n, cube))
m = int(input('Enter another integer:\n'))
print('{0} + {1} is {2}'.format(n,m,n+m))
print('{0} * {1} is {2}'.format(n,m,n*m))
